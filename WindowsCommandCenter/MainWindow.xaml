<Window x:Class="WindowsCommandCenter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WindowsCommandCenter"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Icon="/icon.png">
    <Grid>
        <Slider Name="brightnessSlider" Minimum="0" Maximum="100" SmallChange="1" LargeChange="1" HorizontalAlignment="Left" Margin="34,43,0,0" VerticalAlignment="Top" Width="307" Style="{StaticResource MainSliderStyle}" PreviewMouseUp="brightnessSlider_PreviewMouseUp"/>
        <Label Name="brightnessLabel" Content="Label" HorizontalAlignment="Left" Margin="303,68,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
